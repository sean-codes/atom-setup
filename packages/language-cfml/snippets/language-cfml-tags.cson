    'prefix': 'cflocation'
    'body': '<cflocation url = "$1">$0'
  'cflock':
    'prefix': 'cflock'
    'body': '<cflock${1: timeout = "${2:30}" scope = "${3:session}" name = "${4:[ ]}"  throwOnTimeout = "${5:no}" type = "${6:exclusive}"}>\r\n\t$7\n</cflock>$0'

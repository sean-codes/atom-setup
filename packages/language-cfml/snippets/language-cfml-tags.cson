    'prefix': 'cfexecute'
    'body': '<cfexecute name = "$1"${2: arguments = "${3:[ ]}" outputFile = "${4:[ ]}" variable = "${5:[ ]}" timeout = "${6:[ ]}"}>\r\n\t$7\n</cfexecute>$0'
  'cfexit':
    'prefix': 'cfexit'
    'body': '<cfexit${1: method = "${2:[ ]}"}>$0'

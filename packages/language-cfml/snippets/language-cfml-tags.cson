    'body': '<cfloop collection = "$1" item = "$2">\n\t$3\n</cfloop>$0'
  'cfloop (conditional loop)':
    'prefix': 'cfloop'
    'body': '<cfloop condition = "$1">\n\t$2\n</cfloop>$0'
  'cfloop (date or time loop)':
    'prefix': 'cfloop'
    'body': '<cfloop from = "${1:#createdate($2)#}" to = "${3:#createdate($4)#}" index = "${5:i}"${6: step = "${7:[ ]}"}>\n\t$8\n</cfloop>$0\r\r'

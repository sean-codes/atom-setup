    'prefix': 'cflogin'
    'body': '<cflogin${1: idletimeout = "${2:1800}" applicationToken = "${3:[ ]}" cookieDomain = "${4:[ ]}"}>\n\t<cfloginuser name = "$5" password = "$6" roles = "${7:[ ]}">$8\n</cflogin>$0'
